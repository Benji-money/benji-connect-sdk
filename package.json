{
    "name": "@benji-money/connect-sdk",
    "version": "1.0.1",
    "description": "Benji Connect SDK for authentication and verification",
    "main": "dist/connect-sdk.js",
    "module": "dist/connect-sdk.js",
    "browser": "dist/connect-sdk.umd.js",
    "types": "dist/connect-sdk.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build:babel": "babel src -d dist && cp src/*.d.ts dist/",
        "build:umd": "rollup -c",
        "build": "npm run build:babel && npm run build:umd && cp dist/connect-sdk.umd.js test/connect-sdk.js",
        "build:watch": "chokidar 'src/**/*.js' -c 'npm run build'",
        "serve": "serve ./test",
        "dev": "concurrently \"npm run build:watch\" \"npm run serve\"",
        "prepare": "npm run build"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/benji-money/benji-connect-sdk.git"
    },
    "keywords": [
        "benji",
        "authentication",
        "sdk"
    ],
    "author": "Benji Money",
    "license": "UNLICENSED",
    "devDependencies": {
        "@babel/cli": "^7.23.9",
        "@babel/core": "^7.23.9",
        "@babel/preset-env": "^7.23.9",
        "@rollup/plugin-babel": "^6.0.4",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "chokidar-cli": "^3.0.0",
        "concurrently": "^9.1.2",
        "rollup": "^2.79.2",
        "rollup-plugin-babel": "^4.4.0",
        "serve": "^14.2.4"
    }
}
